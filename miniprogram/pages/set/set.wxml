<!--pages/set/set.wxml-->
<nav-bar navbar-data='{{nvabarData}}'></nav-bar>
<view style='margin-top: {{height}}px;'>
    <view class="weui-flex__item">
        <text class='title'>设置</text>
        <view class="section section_gap">
            <scroll-view class="scroll-view_H" scroll-x style="width: 100%;height:100%;" scroll-with-animation="true">
                <view class="scroll-view-item_H Illustrations about" bindtap='intoThis' data-type='about'>
                    <image src="" mode='aspectFill' style='width:100%;height:100%; border-radius:15px;overflow:hidden;'></image>
                    <view class='v_info'>
                        关于我
                    </view>
                </view>
                <view class="scroll-view-item_H Illustrations usage" bindtap='intoThis' data-type='use'>
                    <image src="" mode='aspectFill' style='width:100%;height:100%; border-radius:15px;overflow:hidden;'></image>
                    <view class='v_info'>
                        使用须知
                    </view>
                </view>
                <button class="scroll-view-item_H Illustrations feedback"  data-type='feedback'  open-type="contact">
                    <image src="" mode='aspectFill' style='width:100%;height:100%; border-radius:15px;overflow:hidden;'></image>
                    <view class='v_info'>
                        反馈交流
                    </view>
                </button>
            </scroll-view>
        </view>
        <i-cell-group>
            <i-cell title="更换皮肤" is-link url="/pages/skin/skin">
                <i-icon type="shop_fill" slot="icon" />
            </i-cell>
            <i-cell title="权限服务" is-link url="/pages/permission/permission">
                <i-icon type="browse_fill" slot="icon" />
            </i-cell>
            <i-cell title="清空缓存" is-link  bindtap='handleOpen'>
                <i-icon type="trash_fill" slot="icon" />
            </i-cell>
            <i-cell title="用户协议" is-link url="">
                <i-icon type="group_fill" slot="icon" />
            </i-cell>
            <i-cell title="更新记录" is-link url="/pages/updateRecord/index">
                <i-icon type="activity_fill" slot="icon" />
            </i-cell>
            <i-cell title="联系我" is-link url="/pages/link/link">
                <i-icon type="interactive_fill" slot="icon" />
            </i-cell>
			 <i-cell title="sm.ms图床" is-link url="/pages/sm/sm">
                <i-icon type="emoji_fill" slot="icon" />
            </i-cell>
            <i-cell title="约稿" wx:if='{{isAdmin}}' is-link url="/pages/update/update?visible=true">
                <i-icon type="activity_fill" slot="icon" />
			</i-cell>
			 <i-cell title="发送模板消息" wx:if='{{isAdmin}}' is-link url="/pages/sendMessage/sendMessage?visible=true">
                <i-icon type="activity_fill" slot="icon" />
			</i-cell>
			 <i-cell title="OkSplash"  bindtap='turnOkSplash'  is-link url="">
                <i-icon type="integral_fill" slot="icon" />
			</i-cell>
			<i-cell title="unsplash" is-link url="/pages/unsplash/unsplash">
                <i-icon type="camera_fill" slot="icon" />
			</i-cell>
        </i-cell-group>
		<!-- <view bindtap='turnOkSplash' style='padding:10px 20px;font-size:14px;'>OkSplash</view> -->
		<!-- <button open-type="contact">反馈</button> -->
        <i-modal visible="{{ visible }}" bind:ok="handleClose2" bind:cancel="handleClose1">
            <view>确定要清空缓存么</view>
        </i-modal>
    </view>
</view>
