.wallpaperList image {
  width: 80%;
  /* height: 100%; */
  background-size: cover;
  border-radius: 10px;
  margin: 0 auto;
}

.wallpaperList .type {
  position: absolute;
  bottom: 10rpx;
  left: 50%;
  margin-left: -40px;
  width: 80px;
  height: 30px;
  background: skyblue;
  border-radius: 15px;
  color: white;
  box-shadow: 3px 3px 3px lightgray;
  text-align: center;
  line-height: 30px;
  font-size: 14px;
  cursor: pointer;
}

