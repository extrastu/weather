.wallpaper {
  background: #2c2c2c;
}

.wallpaperList,
.oneStyle {
  text-align: center;
  width: 100%;
  position: relative;
  display: inline-block;
  vertical-align: top;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  border-radius: 5rem;
  margin: 10px 0px;
}

