.title {
  text-align: center;
  color: #fff;
  font-size: 20px;
  width: 100%;
  margin: 20px 0;
}

.wallpaperList {
  text-align: center;
  width: 100%;
  position: relative;
  display: inline-block;
  vertical-align: top;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  border-radius: 5rem;
  margin: 10px 0px;
}

.wallpaperList image {
  width: 80%;
  /* height: 100%; */
  background-size: cover;
  border-radius: 10px;
  margin: 0 auto;
}
.footer {
